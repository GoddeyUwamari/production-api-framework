# Application
NODE_ENV=production
PORT=3000
API_VERSION=v1

# Database
DB_HOST=your-rds-endpoint.rds.amazonaws.com
DB_PORT=5432
DB_NAME=api_db
DB_USER=dbadmin
DB_PASSWORD=CHANGE_ME_TO_SECURE_PASSWORD
DB_SSL=true
DATABASE_URL=postgresql://dbadmin:password@host:5432/api_db?sslmode=require

# Redis
REDIS_HOST=redis
REDIS_PORT=6379
REDIS_PASSWORD=

# JWT
JWT_SECRET=CHANGE_ME_TO_SECURE_JWT_SECRET

# AWS
AWS_REGION=us-east-1

# Logging
LOG_LEVEL=info
